<% if current_user %>
  <%= render 'shared/add_content' %>
  <%= render 'shared/my_actions' %>
  <%= link_to 'Browse', catalog_index_path, class: 'btn btn-primary about', role: 'menuitem' %>
  <%= render 'shared/list_pages' %>
  <%= link_to 'Help!', new_help_request_path, class: 'btn btn-primary request-help', role: 'menuitem' %>
 <% else %>
  <%= link_to 'Log In', new_user_session_path, class: 'btn btn-primary login', role: 'menuitem' %>
  <%= link_to 'Browse', catalog_index_path, class: 'btn btn-primary about', role: 'menuitem' %>
  <%= render 'shared/list_pages' %>
  <%= link_to 'Help!', new_help_request_path, class: 'btn btn-primary request-help', role: 'menuitem' %>
 <% end %>
